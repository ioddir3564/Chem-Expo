<script>
    var reg=document.querySelector("#regents")
    var ap=document.querySelector("#ap")
    var regBtn = document.querySelector("#reg-btn")
    var apBtn = document.querySelector("#ap-btn")
    function showReg() {
        reg.style.display="block"
        ap.style.display="none"
        console.log(regBtn);
        console.log(apBtn);
        apBtn.classList.remove("current")
        regBtn.classList.add("current")
        regBtn.classList.remove("btn1")
        apBtn.classList.add("btn1")
        console.log(regBtn);
        console.log(apBtn);
    }

    function showAp() {
        ap.style.display="block"
        reg.style.display="none"
        console.log(regBtn);
        console.log(apBtn);
        regBtn.classList.remove("current")
        apBtn.classList.add("current")
        apBtn.classList.remove("btn1")
        regBtn.classList.add("btn1")
        console.log(regBtn);
        console.log(apBtn);
    }

    showReg()


</script>
